  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:hmr": "ng serve --hmr -e=hmr",
    "serve:hmr.proxy": "ng serve --hmr -e=hmr  --proxy-config proxy.conf.json",
    "build": "ng build --prod --build-optimizer",
    "proxy": "ng serve --proxy-config proxy.conf.json",
    "proxy.port": "ng serve --proxy-config proxy.conf.json --port 4400  --live-reload false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "ng test -sr",
    "test-coverage": "ng test -sr -cc",
    "chore.build": "ng build --prod --build-optimizer --bh ./",
    "chore.gh-pages": "run-s chore.build chore.deploy",
    "chore.deploy": "gh-pages -d dist",
    "chore.alain.pull": "git subtree pull --prefix=src/styles/alain alain master",
    "chore.alain.push": "git subtree push --prefix=src/styles/alain alain master",
    "pull": "git  pull origin  qianduan-houtai",
    "push": "git  push origin  qianduan-houtai"
